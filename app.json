{
  "expo": {
    "name": "Daily Glow",
    "slug": "daily-glow",
    "scheme": "daily-glow",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dailyglow.app",
      "associatedDomains": [
        "applinks:daily-glow.app",
        "applinks:*.supabase.co"
      ],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.dailyglow.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "daily-glow"
            },
            {
              "scheme": "https",
              "host": "*.supabase.co",
              "pathPrefix": "/auth/v1/callback"
            },
            {
              "scheme": "https",
              "host": "*.supabase.co",
              "pathPrefix": "/auth/v1/verify"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "newArchEnabled": true,
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "63152758-fdae-4d0e-94b6-4cbb3d8d697f"
      }
    },
    "owner": "ccandelora",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/ccandelora/daily-glow"
    }
  }
}
